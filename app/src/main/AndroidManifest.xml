<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.hp0331.asta">

    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN"/>
    <uses-permission android:name="android.permission.BLUETOOTH"/>
    <uses-permission android:name="android.permission.USE_FINGERPRINT"/>
    <uses-permission android:name="android.permission.WRITE_SETTINGS"/>

    <android:uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <android:uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <android:uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <service
            android:name=".music.MusicPlayerService"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="player"/>
            </intent-filter>
        </service>

        <activity android:name=".login.LoginActivity">
        </activity>
        <activity android:name=".login.GestureEditActivity">
        </activity>
        <activity android:name=".coordinator.CoordinatorActivity">
        </activity>
        <activity android:name=".timecount.TimeCountActivity">
        </activity>
        <activity android:name=".MainActivity">
        </activity>
        <activity android:name=".music.MusicActivity">
        </activity>
        <activity android:name=".login.GestureVerifyActivity">
        </activity>
        <activity android:name=".music.MusicSpeciesActivity">
        </activity>
        <activity android:name=".TenStory.StoryOneActivity">
        </activity>
        <activity android:name=".bannerview.BannerActivity">
        </activity>
        <activity android:name=".games.GamesListActivity">
        </activity>
    </application>

</manifest>